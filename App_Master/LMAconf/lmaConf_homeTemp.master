<body class="stretched no-transition">
<form id="form1" runat="server" style="margin-bottom:0">
<asp:ContentPlaceHolder ID="JavaScript2" runat="server">
	</asp:ContentPlaceHolder>
<!-- Document Wrapper
	============================================= -->
		<div id="wrapper" class="clearfix">

		<!-- Top Bar
		============================================= -->
		<div id="top-bar">

			<div class="container clearfix">

				<div class="col_half nobottommargin">

					<!-- Top Links
					============================================= -->
					<div class="top-links">
						
					</div><!-- .top-links end -->

				</div>

				<div class="col_half fright col_last nobottommargin">

				</div>

			</div>

		</div><!-- #top-bar end -->

		<!-- Header
		============================================= -->
		<header id="header" class="sticky-style-2">

			<div class="container clearfix">

				<!-- Logo
				============================================= -->
				<div id="logo">
				<asp:ContentPlaceHolder ID="logo" runat="server">
    </asp:ContentPlaceHolder> 
				</div><!-- #logo end -->

				<div class="header-extras-date">
						
										<asp:ContentPlaceHolder ID="headerdate" runat="server">
    </asp:ContentPlaceHolder> 
							
					

			</div>
			</div>

			<div id="header-wrap">

				<!-- Primary Navigation
				============================================= -->
				<nav id="primary-menu" class="style-2">

					<div class="container clearfix">

						<div id="primary-menu-trigger"><i class="icon-reorder"></i></div>

				 <asp:ContentPlaceHolder ID="navholder" runat="server">
    </asp:ContentPlaceHolder>

						

						<!-- Top Search
						============================================= -->
						<div id="top-search">
							<a href="#" id="top-search-trigger"><i class="icon-search3"></i><i class="icon-line-cross"></i></a>
							
							<div class="vcosch"><input type="text" name="q" class="form-control" value="" placeholder="Type here and click enter..."></div>
							
						</div>
						<script type="text/javascript">
							$( document ).ready(function() {
$(".form-control").keyup(function(event){
    if(event.keyCode == 13){
                    event.preventDefault();
            var q = $('.form-control').val().replace("?", "%3F");

            location.href = '/events/lma-conf/search-results/#/' + q + "/page=1";

    }
});
});
						</script><!-- #top-search end -->

					</div>

				</nav><!-- #primary-menu end -->

			</div>

		 </header>
  <!-- #header end --> 
  
  <!-- Page Sub Menu
		============================================= -->
  <div id="page-menu">
