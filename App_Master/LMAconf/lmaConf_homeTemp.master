           				<asp:ContentPlaceHolder ID="volunteers" runat="server">
    </asp:ContentPlaceHolder> 
            </div>
          </div>
        </div>
      </div>
      <!-- Testimonies and Volunteer Updates end -->
      
      <div class="line bottommargin-lg"></div>
      
      <!-- Powered by and sponsors begin -->
	  <!-- begin not displaying powered by & sponsor row --><div style="display: none;">
      <div class="container clearfix">
        <div class="col-md-6">
          <div class="heading bottommargin-sm text-center">       				<asp:ContentPlaceHolder ID="power" runat="server">
    </asp:ContentPlaceHolder> 
           
</div>
        </div>
        <div class="col-md-6 col_last">
          <div class="heading bottommargin-sm text-center">       				<asp:ContentPlaceHolder ID="sponsor" runat="server">
    </asp:ContentPlaceHolder> 
</div>
