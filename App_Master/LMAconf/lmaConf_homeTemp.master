      <!-- Action icons end-->
      
      <div class="line bottommargin-lg"></div>
     
      
      <!-- Video begins -->
      
      <div class="container clearfix bottommargin-sm">
        <div class="col-md-6">				<asp:ContentPlaceHolder ID="vidL" runat="server">
    </asp:ContentPlaceHolder> 
 </div>
        <div class="col-md-6 col_last">
                 				<asp:ContentPlaceHolder ID="vidR" runat="server">
    </asp:ContentPlaceHolder>     
        </div>
      </div>
      
